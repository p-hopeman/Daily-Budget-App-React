{
  "expo": {
    "name": "Daily Budget App - React",
    "slug": "daily-budget-app-react",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.philipp.dailybudgetapp.react",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.philipp.dailybudgetapp.react",
          "group.com.philipp.dailybudgetapp.react"
        ]
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.philipp.dailybudgetapp.react"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "abb3a3c9-9b64-48e7-a172-e99157b89eb3"
      }
    }
  }
}
